<?xml version="1.0" encoding="utf-8"?>
<issue:issuetracker
        xmlns="urn:x-ns:hotjazz:issue"
        xmlns:hotjazz="urn:x-ns:hotjazz"
        hotjazz:type="model"
        xmlns:issue="urn:x-ns:hotjazz:issue">
    <issue:name hotjazz:type="SlugField">forge-jira</issue:name>
    <issue:display_name hotjazz:type="CharField">Forge Jira</issue:display_name>
    <issue:tracker_type hotjazz:type="CharField">jira</issue:tracker_type>
    <issue:url hotjazz:type="CharField">https://jira.dev.domain.com/</issue:url>
    <issue:issue_url_pattern hotjazz:type="CharField">https://jira.dev.domain.com/browse/%(issue_id)s</issue:issue_url_pattern>
</issue:issuetracker>

